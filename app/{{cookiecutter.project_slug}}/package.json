{
  "name": "cookiecutter-vue3-template",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit:watch": "vitest watch",
    "test:unit": "vitest run",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false --skipLibCheck",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@babel/types": "^7.23.5",
    "@vueuse/components": "^10.3.0",
    "@vueuse/core": "^10.3.0",
    "axios": "^1.4.0",
    "http-status-codes": "^2.2.0",
    "pinia": "^2.1.3",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-country-flag-next": "^2.3.2",
    "vue-currency-input": "^3.0.5",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.2",
    "vuetify": "^3.4.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.4.2",
    "@mdi/font": "^7.2.96",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.32.4",
    "vue-tsc": "^1.6.5"
  }
}